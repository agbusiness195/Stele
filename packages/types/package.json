{
  "name": "@nobulex/types",
  "version": "2.0.0",
  "description": "Shared TypeScript type definitions and protocol schemas",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/nobulex.git",
    "directory": "packages/types"
  },
  "homepage": "https://nobulex.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/nobulex/issues"
  },
  "author": "Nobulex Labs, Inc.",
  "dependencies": {
    "@nobulex/crypto": "2.0.0"
  },
  "keywords": [
    "nobulex",
    "covenant",
    "ai-accountability",
    "types",
    "typescript",
    "protocol",
    "schema"
  ],
  "module": "./dist/index.mjs"
}
