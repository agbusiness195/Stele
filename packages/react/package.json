{
  "name": "@stele/react",
  "version": "0.1.0",
  "description": "React hooks and reactive primitives for Stele-powered UIs",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@stele/ccl": "0.1.0",
    "@stele/core": "0.1.0",
    "@stele/identity": "0.1.0",
    "@stele/sdk": "0.1.0",
    "@stele/store": "0.1.0",
    "@stele/types": "0.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/Stele.git",
    "directory": "packages/react"
  },
  "homepage": "https://stele.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/stele/issues"
  },
  "author": "Stele Labs, Inc.",
  "keywords": [
    "stele",
    "covenant",
    "ai-accountability",
    "react",
    "hooks"
  ],
  "module": "./dist/index.mjs"
}
