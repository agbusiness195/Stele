{
  "name": "@stele/enterprise",
  "version": "0.1.0",
  "description": "Enterprise features for Stele: analytics, governance, payments, certification, and more",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/Stele.git",
    "directory": "packages/enterprise"
  },
  "homepage": "https://stele.dev",
  "author": "Stele Labs, Inc.",
  "keywords": [
    "stele",
    "enterprise",
    "analytics",
    "governance",
    "payments",
    "certification"
  ],
  "module": "./dist/index.mjs"
}
