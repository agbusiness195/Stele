{
  "name": "@stele/sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for embedding Stele into agent frameworks and applications",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@stele/types": "2.0.0",
    "@stele/crypto": "2.0.0",
    "@stele/core": "2.0.0",
    "@stele/ccl": "2.0.0",
    "@stele/identity": "2.0.0",
    "@stele/store": "2.0.0",
    "@stele/verifier": "2.0.0",
    "@stele/negotiation": "2.0.0",
    "@stele/breach": "2.0.0",
    "@stele/reputation": "2.0.0",
    "@stele/proof": "2.0.0",
    "@stele/attestation": "2.0.0",
    "@stele/discovery": "2.0.0",
    "@stele/schema": "2.0.0",
    "@stele/temporal": "2.0.0",
    "@stele/canary": "2.0.0",
    "@stele/gametheory": "2.0.0",
    "@stele/composition": "2.0.0",
    "@stele/antifragile": "2.0.0",
    "@stele/consensus": "2.0.0",
    "@stele/robustness": "2.0.0",
    "@stele/recursive": "2.0.0",
    "@stele/alignment": "2.0.0",
    "@stele/norms": "2.0.0",
    "@stele/substrate": "2.0.0",
    "@stele/derivatives": "2.0.0",
    "@stele/legal": "2.0.0",
    "@stele/enforcement": "2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/Stele.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://stele.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/stele/issues"
  },
  "author": "Stele Labs, Inc.",
  "keywords": [
    "stele",
    "covenant",
    "ai-accountability",
    "sdk",
    "ai-agents",
    "trust",
    "governance"
  ],
  "module": "./dist/index.mjs"
}
