{
  "name": "@kervyx/sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for embedding Kervyx into agent frameworks and applications",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@kervyx/types": "2.0.0",
    "@kervyx/crypto": "2.0.0",
    "@kervyx/core": "2.0.0",
    "@kervyx/ccl": "2.0.0",
    "@kervyx/identity": "2.0.0",
    "@kervyx/store": "2.0.0",
    "@kervyx/verifier": "2.0.0",
    "@kervyx/negotiation": "2.0.0",
    "@kervyx/breach": "2.0.0",
    "@kervyx/reputation": "2.0.0",
    "@kervyx/proof": "2.0.0",
    "@kervyx/attestation": "2.0.0",
    "@kervyx/discovery": "2.0.0",
    "@kervyx/schema": "2.0.0",
    "@kervyx/temporal": "2.0.0",
    "@kervyx/canary": "2.0.0",
    "@kervyx/gametheory": "2.0.0",
    "@kervyx/composition": "2.0.0",
    "@kervyx/antifragile": "2.0.0",
    "@kervyx/consensus": "2.0.0",
    "@kervyx/robustness": "2.0.0",
    "@kervyx/recursive": "2.0.0",
    "@kervyx/alignment": "2.0.0",
    "@kervyx/norms": "2.0.0",
    "@kervyx/substrate": "2.0.0",
    "@kervyx/derivatives": "2.0.0",
    "@kervyx/legal": "2.0.0",
    "@kervyx/enforcement": "2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/kervyx.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://kervyx.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/kervyx/issues"
  },
  "author": "Kervyx Labs, Inc.",
  "keywords": [
    "kervyx",
    "covenant",
    "ai-accountability",
    "sdk",
    "ai-agents",
    "trust",
    "governance"
  ],
  "module": "./dist/index.mjs"
}
