{
  "name": "@usekova/sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for embedding Stele into agent frameworks and applications",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@usekova/types": "2.0.0",
    "@usekova/crypto": "2.0.0",
    "@usekova/core": "2.0.0",
    "@usekova/ccl": "2.0.0",
    "@usekova/identity": "2.0.0",
    "@usekova/store": "2.0.0",
    "@usekova/verifier": "2.0.0",
    "@usekova/negotiation": "2.0.0",
    "@usekova/breach": "2.0.0",
    "@usekova/reputation": "2.0.0",
    "@usekova/proof": "2.0.0",
    "@usekova/attestation": "2.0.0",
    "@usekova/discovery": "2.0.0",
    "@usekova/schema": "2.0.0",
    "@usekova/temporal": "2.0.0",
    "@usekova/canary": "2.0.0",
    "@usekova/gametheory": "2.0.0",
    "@usekova/composition": "2.0.0",
    "@usekova/antifragile": "2.0.0",
    "@usekova/consensus": "2.0.0",
    "@usekova/robustness": "2.0.0",
    "@usekova/recursive": "2.0.0",
    "@usekova/alignment": "2.0.0",
    "@usekova/norms": "2.0.0",
    "@usekova/substrate": "2.0.0",
    "@usekova/derivatives": "2.0.0",
    "@usekova/legal": "2.0.0",
    "@usekova/enforcement": "2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/Stele.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://stele.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/stele/issues"
  },
  "author": "Stele Labs, Inc.",
  "keywords": [
    "stele",
    "covenant",
    "ai-accountability",
    "sdk",
    "ai-agents",
    "trust",
    "governance"
  ],
  "module": "./dist/index.mjs"
}
