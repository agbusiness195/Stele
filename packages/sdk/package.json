{
  "name": "@nobulex/sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK for embedding Nobulex into agent frameworks and applications",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config ../../vitest.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nobulex/types": "2.0.0",
    "@nobulex/crypto": "2.0.0",
    "@nobulex/core": "2.0.0",
    "@nobulex/ccl": "2.0.0",
    "@nobulex/identity": "2.0.0",
    "@nobulex/store": "2.0.0",
    "@nobulex/verifier": "2.0.0",
    "@nobulex/negotiation": "2.0.0",
    "@nobulex/breach": "2.0.0",
    "@nobulex/reputation": "2.0.0",
    "@nobulex/proof": "2.0.0",
    "@nobulex/attestation": "2.0.0",
    "@nobulex/discovery": "2.0.0",
    "@nobulex/schema": "2.0.0",
    "@nobulex/temporal": "2.0.0",
    "@nobulex/canary": "2.0.0",
    "@nobulex/gametheory": "2.0.0",
    "@nobulex/composition": "2.0.0",
    "@nobulex/antifragile": "2.0.0",
    "@nobulex/consensus": "2.0.0",
    "@nobulex/robustness": "2.0.0",
    "@nobulex/recursive": "2.0.0",
    "@nobulex/alignment": "2.0.0",
    "@nobulex/norms": "2.0.0",
    "@nobulex/substrate": "2.0.0",
    "@nobulex/derivatives": "2.0.0",
    "@nobulex/legal": "2.0.0",
    "@nobulex/enforcement": "2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/nobulex.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://nobulex.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/nobulex/issues"
  },
  "author": "Nobulex Labs, Inc.",
  "keywords": [
    "nobulex",
    "covenant",
    "ai-accountability",
    "sdk",
    "ai-agents",
    "trust",
    "governance"
  ],
  "module": "./dist/index.mjs"
}
