{
  "name": "@usekova/gametheory",
  "version": "2.0.0",
  "description": "Game-theoretic analysis with Nash equilibria and mechanism design",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "bundle": "tsup src/index.ts --format cjs,esm",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run --config ../../vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@usekova/crypto": "2.0.0"
  },
  "license": "MIT",
  "author": "Stele Labs, Inc.",
  "homepage": "https://stele.dev",
  "bugs": {
    "url": "https://github.com/agbusiness195/stele/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agbusiness195/Stele.git",
    "directory": "packages/gametheory"
  },
  "keywords": [
    "stele",
    "covenant",
    "ai-accountability",
    "game-theory",
    "mechanism-design",
    "incentives",
    "nash-equilibrium"
  ]
}
